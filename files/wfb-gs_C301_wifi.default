#!/bin/sh

[ "$(cat /tmp/sysinfo/model)" = "Qihoo 360 C301" ] && {
	uci -q batch << EOT
	del wireless.default_radio0.network
	del wireless.default_radio0.encryption
	set wireless.radio0.htmode='VHT20'
	set wireless.radio0.channel='161'
	set wireless.radio0.cell_density='0'
	set wireless.default_radio0.mode='monitor'
	set wireless.default_radio0.ssid='wfb'
	del wireless.radio0.disabled='1'
EOT
uci commit
reload_config
}

exit 0
